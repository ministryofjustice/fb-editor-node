{
  "name": "@ministryofjustice/fb-editor-node",
  "version": "0.0.175",
  "engines": {
    "node": "12.4.0"
  },
  "description": "Form Builder Editor (for Node)",
  "main": "./index.js",
  "scripts": {
    "start": "node bin/start",
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run gulp -- build\" \"webpack\"",
    "build:watch": "npm run gulp -- build:watch",
    "build:fonts": "npm run gulp -- build:fonts",
    "build:fonts:watch": "npm run gulp -- build:fonts:watch",
    "build:images": "npm run gulp -- build:images",
    "build:images:watch": "npm run gulp -- build:images:watch",
    "build:js": "webpack",
    "build:js:watch": "npm run build:js -- --watch",
    "build:css": "npm run gulp -- build:css",
    "build:css:watch": "npm run gulp -- build:css:watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint *.js lib src build test",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "test": "mocha test --recursive",
    "transform": "npm run gulp -- transform",
    "transform:templates": "npm run gulp -- transform:templates",
    "transform:specifications": "npm run gulp -- transform:specifications",
    "gulp": "gulp --color",
    "pre-commit": "npm run build && npm run transform && git add assets templates specifications"
  },
  "author": "Form Builder Team <form-builder-team@digital.justice.gov.uk>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/fb-editor-node.git"
  },
  "license": "MIT",
  "dependencies": {
    "@ministryofjustice/fb-runner-node": "0.1.24",
    "@ministryofjustice/fb-utils-node": "^0.1.8",
    "@ministryofjustice/module-alias": "^1.0.8",
    "ajv": "^6.10.2",
    "autosize": "^4.0.2",
    "cross-env": "^6.0.3",
    "csvtojson": "^2.0.10",
    "deep-equal": "^2.0.1",
    "dragula": "^3.7.2",
    "flat": "^5.0.0",
    "glob-all": "^3.1.0",
    "json2csv": "^4.5.4",
    "jsonpath": "^1.0.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@ministryofjustice/eslint-config-fb": "^1.1.6",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-all": "^1.1.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.0.0",
    "mocha": "^7.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-scss": "^2.0.0",
    "proxyquire": "^2.1.3",
    "sacred-fs": "^1.0.14",
    "sinon": "^8.0.4",
    "sinon-chai": "^3.4.0",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-scss": "^3.13.0",
    "terser-webpack-plugin": "^2.3.1",
    "vinyl-paths": "^3.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "_moduleAliases": {
    "~/fb-editor-node": ".",
    "~/fb-editor-node/test": "./test",
    "~/fb-editor-node/mock": "./test/.mock"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
